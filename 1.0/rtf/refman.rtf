\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf18 char} solve_square_equation({\cf18 int} a, {\cf18 int} b, {\cf18 int} c, {\cf18 double}* x1, {\cf18 double}* x2)\{\par
    {\cf19 if} (c == 0)\par
    \{\par
\par
        {\cf19 if} (b == 0)\par
        \{\par
\par
            {\cf19 if} (a == 0)\par
            \{{\cf20 //a=0 b=0 c=0}\par
                {\cf19 return} {\cf23 'i'};{\cf20 // where is 'i'-infinite number solves of this equation}\par
            \} {\cf19 else}\par
            \{{\cf20 //b=0 c=0}\par
                *x1 = 0;\par
                {\cf19 return} {\cf23 '1'};\par
            \}\par
\par
        \} {\cf19 else}\par
        \{\par
\par
            {\cf19 if}(a == 0)\par
            \{{\cf20 //a=0 c=0}\par
                *x1 = 0;\par
                {\cf19 return} {\cf23 '1'};\par
            \} {\cf19 else}\par
            \{{\cf20 //c=0}\par
                    *x1 = 0;\par
                    *x2 = -b/a;\par
                    {\cf19 return} {\cf23 '2'};\par
            \}\par
\par
        \}\par
\par
    \} {\cf19 else}\par
    \{\par
\par
        {\cf19 if}(b == 0)\par
        \{\par
\par
            {\cf19 if}(a == 0)\par
            \{{\cf20 //a=0 b=0}\par
\par
                {\cf19 return} {\cf23 '0'};\par
\par
            \} {\cf19 else}\par
            \{{\cf20 //b=0}\par
\par
                {\cf19 if}(c > 0)\par
                \{{\cf20 //ax^2+|c|=0}\par
\par
                    {\cf19 return} {\cf23 '0'};\par
\par
                \} {\cf19 else}\par
                \{{\cf20 //ax^2-|c|=0}\par
\par
                    *x1 = -sqrt(-c);\par
                    *x2 = sqrt(-c);\par
                    {\cf19 return} {\cf23 '2'};\par
\par
                \}\par
\par
            \}\par
\par
        \} {\cf19 else}\par
        \{\par
\par
            {\cf19 if}(a == 0)\par
            \{{\cf20 //a=0}\par
\par
                *x1 = -c/b;\par
                {\cf19 return} {\cf23 '1'};\par
\par
            \} {\cf19 else}\par
            \{{\cf20 //nothing = 0}\par
\par
                *x1 = ( - b - sqrt( b * b - 4 * a * c ) ) / (2 * a);\par
                *x2 = ( - b + sqrt( b * b - 4 * a * c ) ) / (2 * a);\par
                {\cf19 return} {\cf23 '2'};\par
\par
            \}\par
\par
        \}\par
\par
    \}\par
\par
\}\par
}
