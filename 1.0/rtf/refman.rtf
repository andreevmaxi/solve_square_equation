\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf18 char} print_equation({\cf18 int}* a, {\cf18 int}* b, {\cf18 int}* c)\{\par
\par
    {\cf18 int} i = 0;\par
    {\cf18 int} tmp_coefficient = 0;\par
    {\cf18 bool} sign_flag = 0;\par
\par
    tmp = user_equation[i];\par
\par
    {\cf19 while}(tmp!={\cf23 '='} && tmp!={\cf23 '\\n'})\par
    \{\par
\par
\par
        {\cf19 if}(tmp != {\cf23 ' '} && tmp != {\cf23 '+'} && tmp != {\cf23 '-'})\par
        \{\par
          \\defgroup third_module in program module\par
    \\ingroup main_module  {\cf19 if}(tmp != {\cf23 'x'} && tmp != {\cf23 '*'})\par
            \{\par
                tmp_coefficient = tmp_coefficient*10 + (tmp - {\cf23 '0'});\par
\par
            \} {\cf19 else} \{\par
                {\cf19 if}(tmp_coefficient == 0)\{\par
                    ++tmp_coefficient;\par
                \}\par
                {\cf19 if}(tmp == {\cf23 '*'})\{\par
                    ++i;\par
                \}\par
\par
                {\cf19 if}(user_equation[i+1] == {\cf23 '^'})\{\par
\par
                    {\cf19 if} (sign_flag == 0)\{\par
                        *a += tmp_coefficient;\par
                    \} {\cf19 else}\par
                    \{\par
                        *a -= tmp_coefficient;\par
                    \}\par
\par
                    sign_flag = 0;\par
                    i += 2;\par
                    tmp_coefficient = 0;\par
                \}{\cf19 else}\par
                \{\par
                    {\cf19 if} (sign_flag == 0)\{\par
                        *b += tmp_coefficient;\par
                    \} {\cf19 else}\par
                    \{\par
                        *b -= tmp_coefficient;\par
                    \}\par
\par
                    sign_flag = 0;\par
                    tmp_coefficient = 0;\par
                \}\par
\par
            \}\par
        \}{\cf19 else}\par
        \{\par
            {\cf19 if} (tmp_coefficient != 0)\{\par
\par
                {\cf19 if} (sign_flag == 0)\{\par
                    *c += tmp_coefficient;\par
                \} {\cf19 else}\par
                \{\par
                    *c -= tmp_coefficient;\par
                \}\par
\par
                sign_flag = 0;\par
                tmp_coefficient = 0;\par
            \}\par
\par
            {\cf19 if} (tmp == {\cf23 '-'})\{\par
                sign_flag = 1;\par
            \}\par
\par
        \}\par
\par
    ++i;\par
    tmp = user_equation[i];\par
\par
    \}\par
\par
    {\cf19 if} (tmp_coefficient!=0)\{\par
\par
        {\cf19 if}(sign_flag == 0)\{\par
            *c += tmp_coefficient;\par
        \} {\cf19 else} \{\par
            *c -= tmp_coefficient;\par
        \}\par
\par
    \}\par
\par
    {\cf19 return} {\cf23 'k'};{\cf20 //where is k == ok}\par
\par
\}\par
}
