\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf18 char} SolveSquareEquation({\cf18 int} a, {\cf18 int} b, {\cf18 int} c, {\cf18 double}* x1, {\cf18 double}* x2)\{\par
    assert(x1 != NULL);\par
    assert(x2 != NULL);\par
\par
    {\cf19 switch}(a)\par
    \{\par
        {\cf19 case} 0:\par
        \{\par
            {\cf19 if} (b == 0 && c == 0)\{\par
                {\cf19 return} {\cf23 'i'};\par
            \}\par
            {\cf19 if} (b == 0 && c != 0)\{\par
                {\cf19 return} {\cf23 '0'};\par
            \}\par
            *x1=-c/b;\par
            {\cf19 return} {\cf23 '1'};\par
        \}\par
        {\cf19 default}:\par
        \{\par
            {\cf18 double} discriminate = 0; \par
            discriminate = b * b - 4 * a * c;\par
            {\cf19 if} ( discriminate >= 0 )\par
            \{\par
                *x1 = (- b - sqrt( discriminate ) )/(2 * a);\par
                *x2 = (- b + sqrt( discriminate ) )/(2 * a);\par
                {\cf19 if} ( discriminate == 0)\par
                \{\par
                    {\cf19 return} {\cf23 '1'};\par
                \} {\cf19 else}\par
                \{\par
                    {\cf19 return} {\cf23 '2'};\par
                \}\par
            \} {\cf19 else}\par
            \{\par
                {\cf19 return} {\cf23 '0'};\par
            \}\par
        \}\par
\par
    \}\par
\par
\}\par
}
